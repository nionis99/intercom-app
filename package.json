{
  "name": "intercom",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint ./app --ext .ts,.tsx",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"./app/**/*.+(js|jsx|ts|tsx|json|css|md)\"",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "clean:ios": "cd ios && xcodebuild clean && cd ../"
  },
  "dependencies": {
    "@hookform/resolvers": "^1.3.5",
    "@react-native-community/async-storage": "^1.12.0",
    "@react-navigation/bottom-tabs": "^5.9.0",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.9.1",
    "@sentry/react-native": "^2.1.0",
    "@types/react-redux": "^7.1.16",
    "awesome-phonenumber": "^2.38.0",
    "axios": "^0.20.0",
    "i18next": "^19.8.7",
    "lodash": "^4.17.20",
    "moment": "^2.29.0",
    "react": "16.13.1",
    "react-hook-form": "^6.10.1",
    "react-i18next": "^11.8.6",
    "react-native": "0.63.4",
    "react-native-actionsheet": "^2.4.2",
    "react-native-appearance": "^0.3.4",
    "react-native-appstate-hook": "^1.0.5",
    "react-native-config": "^1.3.3",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-elements": "^3.3.1",
    "react-native-fast-image": "^8.3.2",
    "react-native-flags-typescript": "^1.0.1",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-keyboard-aware-scroll-view": "0.9.3",
    "react-native-restart": "^0.0.17",
    "react-native-safe-area-context": "^3.1.8",
    "react-native-screens": "^2.11.0",
    "react-native-storage": "^1.0.1",
    "react-native-svg": "^12.1.0",
    "react-native-toast-message": "^1.4.9",
    "react-native-vector-icons": "^8.1.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "yup": "^0.29.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@reduxjs/toolkit": "^1.5.0",
    "@types/jest": "^25.2.3",
    "@types/lodash": "^4.14.161",
    "@types/react": "^16.9.49",
    "@types/react-native": "^0.63.2",
    "@types/react-native-actionsheet": "^2.4.1",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-test-renderer": "^16.9.2",
    "@types/yup": "^0.29.7",
    "@typescript-eslint/eslint-plugin": "^4.9.0",
    "@typescript-eslint/parser": "^4.9.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.13.0",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^4.3.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.4.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "prettier": "^2.1.2",
    "react-native-svg-transformer": "^0.14.3",
    "react-test-renderer": "16.13.1",
    "typescript": "^4.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && tsc --noEmit"
    }
  },
  "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "eslint --cache --fix"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
